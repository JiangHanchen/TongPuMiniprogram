/* pages/welcome/welcome.wxss */
page{
    background-color:#fdd0cf ;
    height: 100%;
}
.welcome-view{
    height: 100%;
    width: 750rpx;
   background-color:#fdd0cf;
}
.welcome-image{
    padding-top: 250rpx;
    padding-left: 255rpx;
    height: 250rpx;
    width: 250rpx;
}
.enter{
    text-align: center;
    
    justify-content: center;
    border-radius: 10rpx;
    line-height: 70rpx;
    margin-top: 200rpx;
    margin-left: auto;
    margin-right: auto;
    height: 70rpx;
    width: 350rpx;
    background-color: #9bbde5;
}

.enter-text{
    font-size: 35rpx;
    font-family: "华文行楷";
}
@font-face {
    font-family: "华文行楷";
    src: url("华文行楷.eot"); /* IE9 */
    src: url("华文行楷.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
    
    url(data:application/x-font-ttf;charset=utf-8;base64,) format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    
    url("华文行楷.svg#华文行楷") format("svg"); /* iOS 4.1- */
    font-style: normal;
    font-weight: normal;
}
.welcome-text1{
    font-family: "华文新魏";
}
@font-face {
    font-family: "华文新魏";
    src: url("华文新魏.eot"); /* IE9 */
    src: url("华文新魏.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
    
    url(data:application/x-font-ttf;charset=utf-8;base64,) format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    
    url("华文新魏.svg#华文新魏") format("svg"); /* iOS 4.1- */
    font-style: normal;
    font-weight: normal;
}
.welcome-text{
    padding-top: 200rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
}
.welcome-text1{
    color: #b83b14;
    font-size: 48rpx;
}

.welcome-text2{
    color: #4f68a0;
}
.welcome-text3{
    color: #d2735a;
    font-family: "仿宋字体";
}
