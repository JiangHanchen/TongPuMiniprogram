/* pages/answer/answer.wxss */
.answer-view1{
    display: flex;
    justify-content: space-between;
    background-color: #b4c7e7;
    height: 80rpx;
    width: 100%;
    display: flex;
}
.answer-view1-text{
    
    line-height: 80rpx;
}
.answer-view1-image1{
    padding-top: 10rpx;
    height: 60rpx;
    width: 60rpx;
}
.answer-view2{
    display: flex;
}
.answer-view1-image2{
    padding-top: 10rpx;
    height: 190rpx;
    width: 190rpx;
}
.answer-button{
    background-color: #b4c7e7;
    height: 60rpx !important;
    width: 60rpx !important;
    border: none;
}
.answer-score{

    position: relative;
}
.answer-score-text{
    width: 150rpx;
    height: 300rpx;
    margin-top: 90rpx;
    margin-left: 300rpx;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    position: absolute;
    
}
.answer-score-image{
    padding-top: 70rpx;
    padding-left: 250rpx;
    height: 250rpx;
    width: 250rpx;
}
.answer-score-text1{
    color: #afabab;
    margin-left: 40rpx;
}
.answer-score-text2{
    color: #7f7f7f;
    font-size: 130rpx;
}
.answer-score-text2{
    font-family: "Cooper Black";
}
@font-face {
    font-family: "Cooper Black";
    src: url("Cooper Black.eot"); /* IE9 */
    src: url("Cooper Black.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
    
    url(data:application/x-font-ttf;charset=utf-8;base64,) format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    
    url("Cooper Black.svg#Cooper Black") format("svg"); /* iOS 4.1- */
    font-style: normal;
    font-weight: normal;
}
.item{
    margin-left: 70rpx;
    margin-right: 70rpx;
    justify-content: space-between;
    display: flex;
    align-items: center;
    margin-top: 55rpx;
    position: relative;
}
.item-image{
    height: 70rpx;
    width: 70rpx;
}
.item-text{
    position: absolute;
}
.view-item{
    display: flex;
    align-items: center;
    justify-content: center;
}
.container{
    padding-right: 20rpx;
    padding-left: 20rpx;
    padding-top: 50rpx;
    padding-bottom: 0;
}
.divLine{
    background: #E0E3DA;
    width: 100%;
    height: 5rpx;
}
.answer-analysis{
    position: relative;
    height: 60rpx;
    padding-top: 10rpx;
    padding-left: 40rpx;
}
.answer-analysis-image{
    height: 60rpx;
    width: 60rpx;
}
.answer-analysis-text{
    padding-left: 10rpx;
    position: absolute;
    width: 200rpx;
    line-height: 60rpx;
}
.item2{
    display: flex;
    position: relative;
}
.view-item2{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 70rpx;
    height: 70rpx;
    display: flex;
    margin-left: 30rpx;
    margin-top: 30rpx;
}
.item2-false-image{
    height: 70rpx;
    width: 70rpx;
}
.item2-false-text1{
    
    position: absolute;
}

.item2-false-text2{
    padding-top: 40rpx;
    width: 600rpx;
    padding-left: 20rpx;
    display: flex;
}
.item2-false-text2-1{
    color: #767171;
}
.item2-false-text2-2{
    color:#548235;
    line-height: 70rpx;
}

